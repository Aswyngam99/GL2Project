<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.materialdesignicons.MaterialDesignIconView?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUp">
   <children>
      <ImageView fitHeight="409.0" fitWidth="710.0" opacity="0.67" pickOnBounds="true">
         <image>
            <Image url="@Image/plexus.jpg" />
         </image>
         <effect>
            <Bloom threshold="0.2" />
         </effect>
      </ImageView>
      <JFXButton buttonType="RAISED" layoutX="532.0" layoutY="352.0" opacity="0.45" prefHeight="35.0" prefWidth="147.0" style="-fx-background-color: #708090;" text="Submit" textFill="#ebedee">
         <font>
            <Font name="Century Gothic Bold" size="14.0" />
         </font>
      </JFXButton>
      <Label layoutX="691.0" layoutY="1.0" onMouseClicked="#handleClose" text="X" textFill="#9ebce8">
         <font>
            <Font size="18.0" />
         </font>
         <effect>
            <Bloom threshold="0.12" />
         </effect>
      </Label>
      <MaterialDesignIconView fill="#6ca3f1" glyphName="LOGOUT" layoutX="30.0" layoutY="380.0" onMouseClicked="#GoLogin" size="30">
         <effect>
            <Bloom />
         </effect>
      </MaterialDesignIconView>
      <AnchorPane layoutX="60.0" layoutY="61.0" opacity="0.75" prefHeight="258.0" prefWidth="577.0" style="-fx-background-color: #C0C0C0;">
         <children>
            <JFXTextField focusColor="#3391a4" layoutX="20.0" layoutY="25.0" prefHeight="25.0" prefWidth="203.0" promptText="Full name" unFocusColor="#848990">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font>
            </JFXTextField>
            <JFXTextField focusColor="#3391a4" layoutX="20.0" layoutY="104.0" prefHeight="25.0" prefWidth="203.0" promptText="Email" unFocusColor="#848990">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font>
            </JFXTextField>
            <JFXPasswordField focusColor="#3391a4" layoutX="353.0" layoutY="104.0" prefHeight="25.0" prefWidth="203.0" promptText="Password" unFocusColor="#848990">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font>
            </JFXPasswordField>
            <JFXRadioButton layoutX="485.0" layoutY="210.0" selected="true" text="Learner" textFill="#042e3c">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font>
               <toggleGroup>
                  <ToggleGroup fx:id="Group" />
               </toggleGroup>
               <effect>
                  <Glow />
               </effect>
            </JFXRadioButton>
            <JFXRadioButton layoutX="353.0" layoutY="210.0" text="Instructor" textFill="#042e3c" toggleGroup="$Group">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font>
               <effect>
                  <Glow />
               </effect>
            </JFXRadioButton>
            <JFXTextField focusColor="#3391a4" layoutX="353.0" layoutY="25.0" prefHeight="25.0" prefWidth="203.0" promptText="Username" unFocusColor="#848990">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font>
            </JFXTextField>
            <DatePicker layoutX="14.0" layoutY="205.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
